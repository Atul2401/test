pipeline {
    agent any

    stages {
        stage('connection') {
            steps {
                sh '''Echo "Build Step with shell command" '''
            }
        }
        stage('Build Step') {
            steps {
                sh '''cat index.html
                echo "Hello there , You are inside 2nd job"
                echo "NOw we willPrint pyhton file "
                cat abc.py 
                ls -al 
                '''
            }
        }
    }
}
